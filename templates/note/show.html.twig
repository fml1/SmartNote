<div class="ibox ">
    <div class="ibox-title">
        <h5>Просмотр заметки {% if item.title is defined %}{{ item.title }}{% endif %}</h5>
        <div class="ibox-tools">
            <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
            </a>
        </div>
    </div>
    <div class="ibox-content">
    {% if item is defined %}
        <div class="note__show-container">
            {{ item.attrContent|raw }}
        </div>
    {% endif %}
    </div>
</div>
